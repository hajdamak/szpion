{
  "name": "szpion",
  "version": "0.2",
  "description": "Daily Sprint Invigilation",
  "author": "Rafa≈Ç Hajdacki",
  "license": "MIT",
  "homepage": "https://github.com/rhajdacki/szpion#readme",
  "main": "server/main.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/rhajdacki/szpion"
  },
  "scripts": {
    "dev": "yarn dev-compile-watch & yarn dev-client-watch & yarn dev-server-watch & yarn dev-server-start",
    "dev-server": "yarn dev-server-watch & yarn dev-server-start",
    "dev-server-start": "supervisor -w build/server build/server/main.js",
    "dev-compile-watch": "tsc --watch",
    "dev-server-watch": "parcel watch --target node --out-dir ./build/server ./source/server/main.ts",
    "dev-client-watch": "parcel serve --target browser --out-dir ./build/client ./source/client/index.html",
    "start-prod": "echo Not_implemented && exit 1",
    "build": "cross-env NODE_ENV=production parcel build src/index.html --public-url ."
  },
  "dependencies": {
    "@hyperapp/html": "^1.1.1",
    "@types/koa": "^2.0.46",
    "@types/koa-router": "^7.0.31",
    "bulma": "0.7.1",
    "hyperapp": "^1.2.8",
    "koa": "^2.5.2",
    "koa-better-http-proxy": "^0.2.4",
    "koa-router": "^7.4.0",
    "node-sass": "^4.9.3",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "cross-env": "^5.1.6",
    "parcel-bundler": "^1.9.7",
    "supervisor": "^0.12.0",
    "typescript": "^3.0.1"
  }
}
